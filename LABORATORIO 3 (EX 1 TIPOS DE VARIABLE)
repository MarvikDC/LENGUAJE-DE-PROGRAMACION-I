#include <iostream>

using namespace std;

int main()
{

    int a=2000;
    char c=a;
    int b=c;

    if (a==b){cout<<"wou, tenemos caracteres gigantes"<<endl;}
    else{cout<<a<<" es diferente de "<<b<<endl;}

    cout<<a<<endl;
    cout<<b<<endl;
    cout<<c<<endl;
    
    cout<<"________NOMBRES ILEGALES____________"<<endl;
    
    
    return 0;
}
